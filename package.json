{
  "name": "rebrowser",
  "version": "0.2.0",
  "private": true,
  "homepage": ".",
  "main": "./build/electron/main.js",
  "repository": "https://github.com/nicolaslopezj/rebrowser",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dot-object": "^2.1.4",
    "electron-builder-notarize": "^1.5.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-app-events": "^2.0.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "simple-react-form": "^4.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://127.0.0.1:5149 && tsc -p electron -w\" \"wait-on http://127.0.0.1:5149 && tsc -p electron && electron .\"",
    "release": "rm -rf ./dist && yarn build && tsc -p electron && electron-builder --config .electron-builder.config.js --win --mac",
    "start": "PORT=5149 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dot-object": "^2.1.2",
    "@types/lodash": "^4.14.195",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.1.0",
    "electron-builder": "^23.6.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "wait-on": "^7.0.1"
  }
}
